<% include ../partials/header %>

<!-- watch the specific episode -->
<br><br><br>
<h1>Show specific episodes</h1>
<p><%= episode.name %></p>
<p><%= episode._id %></p>

<!-- blooper attached to this video -->
<% episode.bloopers.forEach(function(blooper){ %>
    <p> <%= blooper.videoType %> </p>
    <p> <%= blooper.name %> </p>
    <p> <%= blooper.description %> </p>
<% }) %>

<% include ../partials/footer %>